{
  "parser": "babel-eslint",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "settings": {
        "ecmascript": 6,
        "jsx": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "strict": 0,
        "quotes": 0,
        "no-unused-vars": 0,
        "camelcase": 0,
        "no-underscore-dangle": 0
    },
    "parserOptions": {
       "ecmaVersion": 6,
       "sourceType": "module",
       "ecmaFeatures": {
        "jsx": true
       }
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"]
}
